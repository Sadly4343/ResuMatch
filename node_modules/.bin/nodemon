#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules/nodemon/bin/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules/nodemon/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/node_modules:/Users/kaitlynkryzanski/Desktop/node_modules:/Users/kaitlynkryzanski/node_modules:/Users/node_modules:/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules/nodemon/bin/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules/nodemon/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/node_modules:/Users/kaitlynkryzanski/Desktop/node_modules:/Users/kaitlynkryzanski/node_modules:/Users/node_modules:/node_modules:/Users/kaitlynkryzanski/Desktop/BYU25/Late Summer/Senior Proj/ResuMatch/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
